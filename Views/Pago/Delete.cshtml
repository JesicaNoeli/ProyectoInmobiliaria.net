@model ProyectoInmobiliaria.Models.Pago

@{
    ViewData["Title"] = "Delete";
}

<h2>Borrar</h2>

<h3>Esta Seguro Que Quiere Eliminar este Pagos?</h3>
<div>
    <h4>Pago</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
           <label>ID Pago</label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdPago)
        </dd>
        <dt>
           <label>Numero De Pago</label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NumPago)
        </dd>
        <dt>
            <label>ID Contrato</label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdContr)
        </dd>
        <dt>
           <label>Fecha De Pago</label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.FechaPago)
        </dd>
        <dt>
           <label>Importe</label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Importe)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="submit" value="Eliminar" class="btn btn-default" /> |
        @Html.ActionLink("Volver", "Index", "Inmuebles");
    </form>
</div>
